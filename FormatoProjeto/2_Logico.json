{
  "projectName": "SistemaUniversidade",
  "databaseName": "universidade_db",
  "version": "1.0",
  "model": {
    "type": "Logical",
    "tables": [
      {
        "tableName": "aluno",
        "columns": [
          { "columnName": "id_aluno", "isPrimaryKey": true },
          { "columnName": "nome" },
          { "columnName": "email" },
          { "columnName": "data_nascimento" }
        ]
      },
      {
        "tableName": "curso",
        "columns": [
          { "columnName": "id_curso", "isPrimaryKey": true },
          { "columnName": "nome_curso" },
          { "columnName": "carga_horaria" }
        ]
      },
      {
        "tableName": "matricula",
        "columns": [
          { "columnName": "id_aluno", "isForeignKey": true, "references": { "table": "aluno", "column": "id_aluno" } },
          { "columnName": "id_curso", "isForeignKey": true, "references": { "table": "curso", "column": "id_curso" } },
          { "columnName": "data_matricula" },
          { "columnName": "status" }
        ]
      }
    ]
  }
}